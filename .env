# .env.example
# Application Configuration
PORT=3333
NODE_ENV=development
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001

# Security Configuration
ALLOWED_API_KEYS=your-api-key-1,your-api-key-2
MAX_BODY_SIZE=1048576

# Rate Limiting Configuration
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=60000

# NetSuite Configuration
NS_SUITEQL_BASE_URL=https://your-account.suitetalk.api.netsuite.com/services/rest/query/v1
NS_CONSUMER_KEY=your_consumer_key
NS_CONSUMER_SECRET=your_consumer_secret
NS_ACCESS_TOKEN=your_access_token
NS_TOKEN_SECRET=your_token_secret
NS_OAUTH_REALM=your_realm
NS_SUBSIDIARY_ID=2

# Database Configuration (for future use)
DATABASE_URL="postgresql://postgres:123@localhost:5434/netsuite_gateway?schema=public"
JWT_SECRET='super-secret-jwt-key'